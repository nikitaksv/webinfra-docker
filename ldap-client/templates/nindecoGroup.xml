<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "../template.dtd">
<template>
	<askcontainer>1</askcontainer>
	<description>New Nindeco Group</description>
	<icon>ldap-user.png</icon>
	<invalid>0</invalid>
	<rdn>cn</rdn>
	<!--<regexp>^ou=People,o=.*,</regexp>-->
	<title>Generic: Nindeco Group</title>
	<visible>1</visible>
	<objectClasses>
	<objectClass id="posixGroup"></objectClass>
	<objectClass id="mailGroup"></objectClass>
	</objectClasses>
	<attributes>
		<attribute id="cn">
			<display>CN</display>
			<onchange>=autoFill(mail;%cn%@nindeco.com)</onchange>
			<order>1</order>
			<page>1</page>
		</attribute>				
		<attribute id="mail">
			<display>Name of a mail distribution list</display>
			<order>2</order>
			<page>1</page>
		</attribute>
		<attribute id="gidNumber">
			<display>GUI Number</display>
			<order>3</order>
			<page>1</page>
			<value>=php.GetNextNumber(/;gidNumber)</value>
		</attribute>
		<attribute id="memberUid">
			<display>Members</display>
			<order>3</order>
			<page>1</page>
			<value><![CDATA[=php.MultiList(/;(objectClass=person);uid)]]></value>
		</attribute>
	</attributes> 
</template>